{
  "data": {
    "files": {
      "X_test": "X_test.csv",
      "X_train": "X_train.csv",
      "X_val": "X_val.csv",
      "y_test": "y_test.csv",
      "y_train": "y_train.csv",
      "y_val": "y_val.csv"
    },
    "input_path": "data/final"
  },
  "ensemble": {
    "auto_weight": true,
    "data_types": [
      "normal",
      "combined"
    ],
    "enabled": true,
    "method": "weighted_average",
    "models": [
      "logistic_regression",
      "random_forest",
      "xgboost"
    ],
    "threshold_optimization": {
      "enabled": true,
      "metric_priority": "f1"
    }
  },
  "experiment": {
    "description": "한국 기업 부실예측 모델링 파이프라인",
    "name": "quick_test_modeling",
    "version": "1.0.0"
  },
  "feature_selection": {
    "enabled": false,
    "lasso": {
      "alphas": [
        0.001,
        0.01,
        0.1,
        1.0,
        10.0,
        100.0
      ],
      "cv_folds": 5,
      "threshold": 1e-05
    }
  },
  "logging": {
    "level": "INFO",
    "save_to_file": true
  },
  "models": {
    "logistic_regression": {
      "C_range": [
        0.01,
        10
      ],
      "enabled": true,
      "l1_ratio_range": [
        0.1,
        0.9
      ],
      "l2_solvers": [
        "newton-cg",
        "lbfgs"
      ],
      "max_iter_range": [
        1000,
        5000
      ],
      "n_trials": 10,
      "penalty": [
        "l2"
      ]
    },
    "random_forest": {
      "enabled": true,
      "max_depth_range": [
        3,
        20
      ],
      "max_features_range": [
        0.1,
        1.0
      ],
      "min_samples_leaf_range": [
        1,
        10
      ],
      "min_samples_split_range": [
        2,
        20
      ],
      "n_estimators_range": [
        50,
        300
      ],
      "n_trials": 10
    },
    "xgboost": {
      "colsample_bytree_range": [
        0.6,
        1.0
      ],
      "enabled": true,
      "learning_rate_range": [
        0.01,
        0.3
      ],
      "max_depth_range": [
        3,
        10
      ],
      "n_estimators_range": [
        50,
        300
      ],
      "n_trials": 10,
      "reg_alpha_range": [
        0.0,
        10.0
      ],
      "reg_lambda_range": [
        1.0,
        10.0
      ],
      "subsample_range": [
        0.6,
        1.0
      ]
    }
  },
  "output": {
    "base_dir": "outputs/modeling_runs",
    "generate_visualizations": true,
    "report_format": [
      "txt",
      "html"
    ],
    "save_models": true,
    "save_results": true
  },
  "performance": {
    "n_jobs": -1
  },
  "random_state": 42,
  "sampling": {
    "data_types": {
      "combined": {
        "description": "SMOTE + 언더샘플링 조합",
        "enabled": true,
        "smote": {
          "k_neighbors": 5,
          "kind": "borderline-1",
          "m_neighbors": 10,
          "sampling_strategy": 0.1
        },
        "undersampling": {
          "method": "random",
          "random_state": 42,
          "sampling_strategy": 0.3
        }
      },
      "normal": {
        "description": "원본 데이터 (샘플링 없음)",
        "enabled": true
      },
      "smote": {
